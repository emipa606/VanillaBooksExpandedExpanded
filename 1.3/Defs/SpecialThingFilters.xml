<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <SpecialThingFilterDef>
    <defName>VanillaBooksExpandedExpanded_AllowCopies</defName>
    <label>allow copied books</label>
    <description>Allow book copies that have been created using a printing press.</description>
    <parentCategory>VBE_Books</parentCategory>
    <allowedByDefault>true</allowedByDefault>
    <saveKey>allowVanillaBooksExpandedExpandedCopies</saveKey>
    <workerClass>VanillaBooksExpandedExpanded.SpecialThingFilterWorker_CopiedBooks</workerClass>
  </SpecialThingFilterDef>
  <SpecialThingFilterDef>
    <defName>VanillaBooksExpandedExpanded_AllowOriginals</defName>
    <label>allow original books</label>
    <description>Allow original books that have been written at a writers' table or typewriter.</description>
    <parentCategory>VBE_Books</parentCategory>
    <allowedByDefault>true</allowedByDefault>
    <saveKey>allowVanillaBooksExpandedExpandedOriginals</saveKey>
    <workerClass>VanillaBooksExpandedExpanded.SpecialThingFilterWorker_OriginalBooks</workerClass>
  </SpecialThingFilterDef>
</Defs>